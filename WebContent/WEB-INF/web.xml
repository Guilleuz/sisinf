<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>sisinf</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <!-- Listado de servlets -->
  <servlet>
    <display-name>Menú Bizi</display-name>
    <servlet-name>menuBizi</servlet-name>
    <servlet-class>es.unizar.sisinf.grpV2_B.servlet.Bizi.servletMenuBizi</servlet-class>
  </servlet>
  
  <servlet>
    <display-name>Estación Bizi</display-name>
    <servlet-name>estacionBizi</servlet-name>
    <servlet-class>es.unizar.sisinf.grpV2_B.servlet.Bizi.datosEstacion</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>menuBizi</servlet-name>
    <url-pattern>/menuBizi</url-pattern>
  </servlet-mapping>
    
  <servlet-mapping>
    <servlet-name>estacionBizi</servlet-name>
    <url-pattern>/estacionBizi</url-pattern> 
  </servlet-mapping>
</web-app>